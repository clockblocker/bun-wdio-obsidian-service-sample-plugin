{
	"author": "",
	"description": "Sample Obsidian plugin using wdio-obsidian-service for e2e tests",
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@types/node": "^18.19.130",
		"@wdio/appium-service": "^9.20.0",
		"@wdio/cli": "^9.20.0",
		"@wdio/local-runner": "^9.20.0",
		"@wdio/mocha-framework": "^9.20.0",
		"@wdio/spec-reporter": "^9.20.0",
		"appium": "^2.19.0",
		"appium-uiautomator2-driver": "^4.2.9",
		"builtin-modules": "^5.0.0",
		"bun-types": "^1.3.4",
		"esbuild": "^0.25.11",
		"obsidian": "^1.10.2",
		"obsidian-launcher": "^2.1.6",
		"tslib": "^2.8.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"wdio-obsidian-reporter": "^2.1.6",
		"wdio-obsidian-service": "^2.1.6"
	},
	"keywords": [],
	"license": "MIT",
	"main": "main.js",
	"name": "obsidian-sample-plugin",
	"scripts": {
		"build": "bun x tsc -noEmit -skipLibCheck && bun run esbuild.config.mjs production",
		"build:dev": "bun x tsc -noEmit -skipLibCheck && bun run esbuild.config.mjs development",
		"dev": "bun run esbuild.config.mjs",
		"format": "bun x biome format --write .",
		"lint": "bun x biome check .",
		"lint:fix": "bun x biome check --write .",
		"test": "bun run test:unit && bun run test:e2e",
		"test:e2e": "bun x wdio run ./wdio.conf.mts",
		"test:unit": "bun test",
		"coverage": "bun test --coverage",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"version": "1.0.0"
}
